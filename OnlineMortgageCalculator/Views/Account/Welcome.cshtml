@model OnlineMortgageCalculator.Models.RegisterUser
@{
	ViewBag.Title = "Welcome";
}

<h2>Welcome @Model.FirstName @Model.LastName</h2>

<div class="row">
	<div class="col-md-6">
		@using (Html.BeginForm("Index", "Calculate", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
		{
			<input type="submit" value="Go to Calculator" class="btn btn-primary" />
		}
	</div>
</div>

